  .slick-next {
    right: -20px; // half of button width
  }

  .slick-prev {
    left: $grid-gutter - 20px; // offset grid negative margin + half of button width
  }

  @include media-query($small) {
    .slick-next {
      right: -10px;
      top: 34%;
