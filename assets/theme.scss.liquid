  .grid__item {
    display: none;

    &:first-child {
      display: block;
    }
  }

  &.slick-initialized .grid__item {
    display: block;
  }
}

// Offset for quick view button

.collection-switcher__collection-grid {
  .slick-list {
    padding-top: 15px;
