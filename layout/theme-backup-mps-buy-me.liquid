    <script defer="defer">

    setTimeout(function() {
      var cells = $(".isp_product_image_wrapper");

      $('.swym-button').each(function (i) {
        cells.eq(i).append($(this));
      });

    }, 2000);

    $(".swatch-gallery").hover(function() {
      var data=$(this).find(".tlf_product_color").html();
      $("#tlf_product_hover_color").hide().stop().html(data).show();
      $("#tlf_color").css("display", "none");
      $(this).addClass("swatch-gallery_hover");

    }, function() {
      $("#tlf_product_hover_color").hide();
      $("#tlf_color").css("display", "block");
      $(this).removeClass("swatch-gallery_hover");

    });
    $(document).ready(function(){

      var a = $('#upsellmain');
      var b = $('#DrawerHeader');
      if (window.innerWidth < 589) {
        b.append(a);
      }
    });
      $(".grid-product__image-mask").hover(function(){
        $(this).find(".upsell-desktop").css("display","block");
      },function(){
        $(this).find(".upsell-desktop").css("display","none");
      });        

    $(".mobile-upsell-button").on('click', function(){
      $(this).parent().find(".upsell-mobile").toggleClass("show");
    });
    $(".quickbuy-box-mobile").on('click', function(){
      $(this).parents().find(".upsell-mobile").removeClass("show");
    });
    
    $('.quick-buy').click(function(e) {
      var variantID = parseInt(e.target.value);
      // Opens-up the cart drawer
      function openDrawer() {
        $('body').trigger('added.ajaxProduct');
        $(".noproducts").css('display', 'none');
      }
      $('body').trigger('added.ajaxProduct');
      jQuery.ajax ({
        url: '/cart/add.js',
        type: 'POST',
        dataType: 'json',
        'contentType': 'application/json',
        data: JSON.stringify({
          quantity: 1,
          id: variantID
        }),
        success: function(){openDrawer();},
      });
    });
    
    var upTop = document.getElementsByClassName('variant-input');

    for (var i=0; i < upTop.length;i++) {
      upTop[i].addEventListener('click', function() {

        var node = document.getElementById('soldout');
        var parent = document.getElementsByClassName('variant-wrapper')[1];
        var available = document.getElementById('available');

        parent.insertAdjacentElement('afterend', node);
        parent.insertAdjacentElement('afterend', available);

        setInterval(function(){
          var AddToCartBtn = document.getElementsByClassName("productbuybutton");
          var BuyButton = document.getElementsByClassName("payment-buttons")[0];
          var quantity = document.getElementById('quantitynumber');

          for (var ii=0; ii < AddToCartBtn.length;ii++) {
            if (AddToCartBtn[ii].innerHTML == "Sold Out") {
              node.style.display = "block";   
              BuyButton.style.display = "none";
              available.style.display = "none";
              quantity.style.display = "none";
            } else { 
              node.style.display = "none";  
              BuyButton.style.display = "block";
              available.style.display = "block";
              quantity.style.display = "block";
            }
          }

        }, 500);
      });
    }

  </script>
